{
  "Serilog": {
    "Using": [
      "Serilog.Sinks.Console",
      "Serilog.Sinks.File"
    ],
    "MinimumLevel": "Debug",
    "WriteTo": [
      {
        "Name": "Console",
        "Args": {
          "theme": "Serilog.Sinks.SystemConsole.Themes.AnsiConsoleTheme::Code, Serilog.Sinks.Console",
          "outputTemplate": "{Timestamp:yyyy-MM-dd HH:mm:ss} [{Level:u3}] {Message:lj}{NewLine}{Exception}"
        }
      },
      {
        "Name": "File",
        "Args": {
          "path": "logs/sync-logs-.log",
          "outputTemplate": "{Timestamp:yyyy-MM-dd HH:mm:ss} ({SourceContext}) [{Level:u3}] {Message}{NewLine}{Exception}{NewLine}",
          "rollingInterval": "Day",
          "retainedFileCountLimit": 30,
          "formatter": "Serilog.Formatting.Json.JsonFormatter, Serilog"
        }
      }
    ],
    "Enrich": [
      "FromLogContext"
    ],
    "Properties": {
      "Application": "CloudSynkr"
    }
  },
  "Logging": {
    "LogLevel": {
      "Default": "Debug",
      "System": "Information",
      "Microsoft": "Information"
    }
  },
  "SyncBackup": {
    "Mappings": [
      {
        "CloudFolderParentId": "1aGNnX_e419AlZPn1BqoI0cKallDKKj2_",
        "CloudFolderParentName": "configs",
        "LocalFolder": "C:\\Projs\\CloudSynkr\\CloudSynkr.App\\Backup 8\\configs\\Backup",
        "CloudFolder": "Backup",
        "ActionType": 0,
        "FilesToSync": [
          "Nintendo - Snes M.lpl",
          "Copy of Copy of Nintendo - Snes M.lpl",
          "Copy of Nintendo - Snes M.lpl"
        ]
      },
      {
        "CloudFolderParentId": "1ULZkRfLe04oMy8XNYd9an7mXeYzDE4PC",
        "CloudFolderParentName": "Retroarch",
        "LocalFolder": "C:\\Projs\\CloudSynkr\\CloudSynkr.App\\Backup 7\\configs\\Backup\\Saves\\Retroarch\\Snes",
        "CloudFolder": "Snes",
        "ActionType": 0,
        "FilesToSync": [
          "1.txt",
          "2.txt",
          "3.txt"
        ]
      }
    ]
  }
}